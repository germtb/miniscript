import { internalKeys, internalGet } from 'InternalDict'
import { map } from 'List'
import { log, pipeLog } from 'Debug'

get = (key, dict) => internalGet(key, dict)

keys = dict => internalKeys(dict)

values = dict => dict |> keys |> map(k => get(k, dict))

entries = dict => dict |> keys |> map(k => [k, get(k, dict)])

module = {
  get,
  keys,
  values,
  entries
}
